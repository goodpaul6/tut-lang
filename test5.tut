extern printf(format : str, ...) : void

struct Entity
{
	x : float;
	y : float;
}

func move_entity(e : ref-Entity, x : float, y : float) : void
{
	var xr : ref-float = &e->x;
	var yr : ref-float = &e->y;

	*xr = x;
	*yr = y;
}

func _main() : void
{
	var e : Entity;

	e.x = 10.0;
	e.y = 10.0;

	printf("before moving %i, %i\n", e.x, e.y);
	move_entity(&e, 10.0, 20.0);
	printf("after moving %i, %i\n", e.x, e.y);
}